    "@types/react-dom": "^17.0.2 || ^18.0.0",
    "@types/react-syntax-highlighter": "^13.5.2",
    "eslint-config-sodaru": "^1.0.1",
    "next": "^12.1.0",
    "next-sitemap": "^4.0.6",
    "prettier-config-sodaru": "^1.0.0",
    "react": "^18.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-sodaru"
    ]
  },
  "prettier": "prettier-config-sodaru",
